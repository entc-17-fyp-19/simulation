      xyz="0 0 1" />
  </joint>

  <joint name="front_RW_joint"
    type="continuous">
    <origin
      xyz="0 0 -0.15"
      rpy="0 ${pi/2} 0" />
    <parent
      link="front_wheel_right_connector" />
    <child
      link="front_wheel_right" />
    <axis
      xyz="0 0 1" />
  </joint>

<!-- "-0.35409 -0.22 -0.13487" -->

  <joint name="front_LW_joint"
    type="continuous">

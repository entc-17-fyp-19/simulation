  <joint name="base_joint" type="fixed">
    <origin
      xyz="0 0 0.22987"
      rpy="0 0 ${-pi}" />
    <parent
      link="base_footprint" />
    <child
      link="chassis" />
  </joint>

  <joint name="back_LW_joint"
    type="continuous">
    <origin
      xyz="0.23591 -0.245 -0.06987"
      rpy="${pi/2} 0 0" />
    <parent
      link="chassis" />
    <child
      link="back_wheel_left" />
    <axis
      xyz="0 0 1" />
  </joint>

  <joint name="back_RW_joint"
    type="continuous">
    <origin
      xyz="0.23591 0.245 -0.06987"
      rpy="${-pi/2} 0 0" />
    <parent
      link="chassis" />
    <child
      link="back_wheel_right" />
    <axis
      xyz="0 0 -1" />
  </joint>

  <joint name="front_LW_con_joint"
    type="revolute">
    <limit effort="30" velocity="${pi/3}" lower="${-pi/2}" upper="${pi/2}"/>
    <origin
      xyz="-0.35409 -0.22 0.02013"
      rpy="0 0 ${pi/2}" />
    <parent
      link="chassis" />
    <child
      link="front_wheel_left_connector" />
    <axis
      xyz="0 0 1" />
  </joint>

  <joint name="front_RW_con_joint"
    type="revolute">
    <limit effort="30" velocity="${pi/3}" lower="${-pi/2}" upper="${pi/2}"/>
